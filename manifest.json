{
  "name": "JetList Task Expander",
  "description": "Type in a task. We will provide the steps to complete it.",
  "version": "1.0",
  "manifest_version": 3,
  "icons": {
    "48": "assets/48.png",
    "72": "assets/72.png",
    "96": "assets/96.png",
    "144": "assets/144.png"
  },
  "action": {
    "default_popup": "index.html",
    "default_title": "Generate a to-do list"
  },
  "permissions": ["contextMenus", "tabs", "storage"]
}
